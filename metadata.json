{
  "name": "kodguru-nfsclient",
  "version": "0.4.0",
  "author": "kodguru",
  "summary": "Manage nfs client",
  "license": "Apache-2.0",
  "source": "git://github.com/kodguru/puppet-module-nfsclient.git",
  "project_page": "https://github.com/kodguru/puppet-module-nfsclient",
